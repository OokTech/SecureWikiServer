{
  "filePathBase": "homedir",
  "serverKeyPath": "./ookprivate.key",
  "certPath": "./ooktest.crt",
  "httpsPort": "8443",
  "wssPort": "40510",
  "tokenPrivateKeyPath": "./.ssh/id_rsa",
  "tokenTTL": "24h",
  "saltRounds": "10",
  "wikiPathBase": "homedir",
  "wikisPath": "./Wikis",
  "indexWikiName": "IndexWiki",
  "access": {
    "wikis": {
      "RootWiki": {
        "Guest": ["view"],
        "Admin": ["view", "edit", "admin"]
      }
    },
    "actions": {
      "view": ["browserTiddlerList", "test", "ping", "pong"],
      "edit": ["saveTiddler", "cancelEditingTiddler", "clearStatus", "deleteTiddler", "editingTiddler"],
      "admin": ["addWiki", "saveSettings", "shutdownServer", "updateRoutes", "buildHTMLWiki", "newWikiFromTiddlers", "createNewWiki", "restartServer"],
      "scripts": ["runScript"]
    }
  }
}
